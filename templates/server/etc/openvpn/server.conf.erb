#
port <%= port %>
proto <%= proto %>
dev <%= dev %>
ca /var/lib/puppet/ssl/certs/ca.crt
cert /var/lib/puppet/ssl/certs/<% ::fqdn %>.pem
key /var/lib/puppet/ssl/private_keys/<% ::fqdn %>.pem
dh dh2048.pem
server 192.168.101.0 255.255.255.0
ifconfig-pool-persist <%= name %>.conf.d/ipp.txt
keepalive 10 120
;tls-auth ta.key 0
cipher BF-CBC
comp-lzo
;user nobody
;group nogroup
persist-key
persist-tun
status /var/log/openvpn/<%= name %>-status.log
log /var/log/openvpn/openvpn.log
verb 3
;mute 20
