#
port <%= port %>
proto <%= proto %>
dev <%= dev %>
ca /var/lib/puppet/ssl/certs/ca.pem
cert /var/lib/puppet/ssl/certs/<%= fqdn %>.pem
key /var/lib/puppet/ssl/private_keys/<%= fqdn %>.pem
dh dh2048.pem
server <%= network %> <%= netmask %>
ifconfig-pool-persist <%= name %>.conf.d/ipp.txt
keepalive <%= keepalive %>
cipher <%= cipher %>
comp-lzo
persist-key
persist-tun
status /var/log/openvpn/<%= name %>-status.log
log /var/log/openvpn/openvpn.log
verb 3

<%- push.sort.each do |key| -%>
push "<%= key %>"
<%- end -%>
