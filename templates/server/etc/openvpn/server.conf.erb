port <%= port %>
proto <%= proto %>
dev <%= dev %>
ca ca.crt
cert <%= name %>.conf.d/server.crt
key <%= name %>.conf.d/server.key
dh <%= name %>.conf.d/dh2048.pem
server 192.168.101.0 255.255.255.0
ifconfig-pool-persist <%= name %>.conf.d/ipp.txt
